var trafficObj; var trafficCnt=0;
var trafficIDArray = new Array();var trafficValueArray = new Array();
function signal_view(context, imgurl, signal_id, signal, year){
	trafficIDArray[trafficCnt]=signal_id;trafficValueArray[trafficCnt]=signal;trafficCnt++;
	if(trafficCnt == 1){signal_flash(context, imgurl, signal_id,signal,"true", year);}else{signal_flash(context, imgurl, signal_id,signal,"false", year);}
}
function signal_run(){
	for(var i=0;i<trafficIDArray.length;i++){
		thisMovie(trafficIDArray[i]).init_Signal(signal_return("VBOX_"+trafficValueArray[i]))
	}
}
function signal_object(arg){trafficObj=arg; signal_run()}
function signal_return(arg){return trafficObj[arg]}
function thisMovie(movieName) {
    if (navigator.appName.indexOf("Microsoft") != -1) {return window[movieName]}
    else {      return document[movieName]  }
}
function signal_flash(context, imgurl, signal_id, signal, bool, year)
{
